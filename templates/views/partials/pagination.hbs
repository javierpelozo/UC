<ul class="pagination masEventosSiguiente">
	<li {{#unless data.posts.previous}}class="disabled"{{/unless}}>
		<a class="eventoAnterior wow fadeInLeft animated" href="{{paginationPreviousUrl data.posts.previous}}"><i class="fa fa-angle-left"></i>Anterior</a>
	</li>
	{{{paginationNavigation data.posts.pages data.posts.currentPage data.posts.totalPages}}}
	<li {{#unless data.posts.next}}class="disabled"{{/unless}}>
		<a class="eventoSiguiente wow fadeInRight animated" href="{{paginationNextUrl data.posts.next data.posts.totalPages}}"><i class="fa fa-angle-right"></i>Siguiente</a>
	</li>
</ul>
